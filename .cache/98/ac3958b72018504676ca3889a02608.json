put\n          s={12}\n          label=\"Restaurant Name\"\n          id=\"restaurantName\"\n          name=\"restaurantName\"\n          error={errors.restaurantName}\n          value={values.restaurantName}\n          onChange={handleChange}\n          data-testid=\"newRestaurantName\"\n          ref={nameInput}\n        />\n      </Row>\n      <Row>\n        <Button\n          type=\"button\"\n          data-testid=\"cancelAddRestaurantButton\"\n          onClick={handleCancel({ resetForm })}\n        >\n          Cancel\n        </Button>\n        <Button\n          type=\"submit\"\n          data-testid=\"saveNewRestaurantButton\" >\n          Save\n        </Button>\n      </Row>\n    </form>\n  );\n\n  return (\n    <Formik\n      initialValues={{ restaurantName: '' }}\n      onSubmit={handleSave}\n      validate={validate}\n      >\n      {renderForm}\n    </Formik>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"9e5620e6f666480bd23883c21f6ad6a3","cacheData":{"env":{}}}